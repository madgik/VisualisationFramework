{
  "name": "data-analytics-visualization-ui",
  "version": "0.0.1",
  "private": false,
  "dependencies": {
    "axios": "~0.18.0",
    "canvas-background": "~0.1.3",
    "canvas-toBlob": "~1.0.0",
    "chart.js": "~2.7.2",
    "d3": "~4.13.0",
    "file-saver": "~1.3.8",
    "immutability-helper": "~2.8.1",
    "leaflet": "~1.3.4",
    "plotly.js": "~1.42.5",
    "react-chartjs-2": "~2.7.4",
    "react-checkbox-list": "0.0.2",
    "react-leaflet": "~2.0.1",
    "react-plotly.js": "~2.2.0",
    "react-redux": "~5.0.7",
    "react-table": "~6.8.6",
    "redux": "~4.0.0",
    "redux-logger": "~3.0.6",
    "redux-thunk": "~2.3.0",
    "semantic-ui-react": "~0.80.2",
    "react-id-generator": "~0.1.6"
  },
  "devDependencies": {
    "babel-install": "~2.1.0",
    "babel-loader": "~7.1.5",
    "babel-plugin-transform-react-remove-prop-types": "~0.4.15",
    "babel-plugin-transform-object-rest-spread": "~6.26.0",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-es2017": "~6.24.1",
    "copyfiles": "~2.1.0",
    "css-loader": "~1.0.0",
    "file-loader": "~2.0.0",
    "html-loader": "~0.5.5",
    "jquery": "3.3.1",
    "react": "~16.5.1",
    "react-dom": "~16.5.1",
    "react-scripts": "~1.1.5",
    "replace": "~1.0.0",
    "replace-bundle-webpack-plugin": "~1.0.0",
    "webpack": "~3.12.0"
  },
  "scripts": {
    "start": "set PORT=3006 && react-scripts start",
    "develop": "webpack --mode development --watch",
    "build": "npm run build:clean-react && npm run build:clean-portlet && npm run build:clean-portlet-assets && npm run build:webpack && npm run build:copy-bundle && npm run build:copy-assets && npm run build:replace-filenames",
    "build:clean-react": "rimraf dist",
    "build:clean-portlet": "rimraf '../data-analytics-visualization-ui/src/main/webapp/static/bundle.*'",
    "build:clean-portlet-assets": "rimraf '../data-analytics-visualization-ui/src/main/webapp/static/assets/*.*'",
    "build:webpack": "webpack -p --config webpack.config.js",
    "build:mvn": "mvn -f ../data-analytics-visualization-ui/pom.xml clean package",
    "build:copy-bundle": "copyfiles -f \"./dist/*.*\" ../data-analytics-visualization-ui/src/main/webapp/static/",
    "build:copy-assets": "copyfiles -f \"./dist/assets/*.*\" ../data-analytics-visualization-ui/src/main/webapp/static/assets/",
    "build:replace-filenames": "node replace-filenames.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
